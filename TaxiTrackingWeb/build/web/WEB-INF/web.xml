<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Usuario_Negocio</servlet-name>
        <servlet-class>Negocio.Usuario_Negocio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmailServlet</servlet-name>
        <servlet-class>Negocio.EmailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletContras</servlet-name>
        <servlet-class>Negocio.ServletContras</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManejoSesion</servlet-name>
        <servlet-class>Sesion.ManejoSesion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Usuario_Negocio</servlet-name>
        <url-pattern>/Usuario_Negocio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmailServlet</servlet-name>
        <url-pattern>/EmailServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletContras</servlet-name>
        <url-pattern>/ServletContras</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManejoSesion</servlet-name>
        <url-pattern>/ManejoSesion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
